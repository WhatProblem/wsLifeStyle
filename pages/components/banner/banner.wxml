<!--pages/components/banner/banner.wxml-->
<!-- <text>pages/components/banner/banner.wxml</text> -->
<swiper class="itemImg" indicator-dots="{{indicatorDots}}" autoplay="true" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}" wx:key="index">
    <swiper-item>
       <image bindtap="toHomeDetail" src="{{item}}" class="slide-image homePoster" width="100%" height="150" /> 
    </swiper-item>
  </block>
</swiper>